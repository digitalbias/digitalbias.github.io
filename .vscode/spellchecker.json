{
    "language": "en_US",
    "ignoreWordsList": [
        "podcast"
    ],
    "documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "ignoreRegExp": [
        "/\\\\(.*\\\\.(jpg|jpeg|png|md|gif|svg|pdf|PDF|SVG|JPG|JPEG|PNG|MD|GIF)\\\\)/g",
        "/((http|https|ftp|git)\\\\S*)/g",
        "/^(```\\\\s*)(\\\\w+)?(\\\\s*[\\\\w\\\\W]+?\\\\n*)(```\\\\s*)\\\\n*$/gm"
    ],
    "ignoreFileExtensions": [
        ".env"
    ],
    "checkInterval": 5000
}